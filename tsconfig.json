{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    //"sourceMap": true,
    "module": "esnext",
    "target": "es2021",
    "jsx": "react",
    "lib": ["es2021"],
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },

  "exclude": ["node_modules"]
}
